{
	"version": 1,
	"disable_existing_loggers": false,
	"formatters": {
		"color": {
			"()": "goo_net_scrape.logging_config.ColorFormatter",
			"format": "%(asctime)s [ %(levelname)-5s ] %(name)-8s : %(message)s",
			"datefmt": "%Y-%m-%d %H:%M:%S"
		},
		"plain": {
			"format": "%(asctime)s [ %(levelname)-5s ] %(name)-8s : %(message)s",
			"datefmt": "%Y-%m-%d %H:%M:%S"
		}
	},
	"handlers": {
		"console": {
			"class": "logging.StreamHandler",
			"level": "DEBUG",
			"formatter": "color",
			"stream": "ext://sys.stdout"
		},
		"file": {
			"class": "logging.FileHandler",
			"level": "DEBUG",
			"formatter": "plain",
			"filename": "history.log",
			"encoding": "utf-8"
		}
	},
	"root": {
		"level": "DEBUG",
		"handlers": ["console", "file"]
	}
}
